<div>
	<%= form_for @joke do |f| %>
		<div>
		<%= f.label :title %>
		<%= f.text_field :title %>
		</div>

		<div>
		<%= f.label :content %>
		<%= f.text_area :content %>
		</div>

		<%= f.label :image_url %>
		<%= f.text_field :image_url %>

		<div>
		<%= f.submit %>
		</div>
	<% end %>
</div>